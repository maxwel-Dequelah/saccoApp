{
  "expo": {
    "name": "saccoApp",
    "plugins": ["expo-router"],
    "slug": "saccoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tomikal",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dequelah.saccoApp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sacco_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dequelah.saccoApp"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiUrl": "http://162.220.14.16:31/",
      "otherEnvVar": "someValue",
      "eas": {
        "projectId": "0b362545-5485-4810-81cc-27344b1a40e1"
      }
    }
  }
}
